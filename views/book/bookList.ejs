<!-- text -->
<div class='text-center'>
    <h1 class='display-1 mt-5 mb-4'>Hello, world!</h1>
  </div>

<!-- table -->
<div class='container'>
    <table class="table table-striped">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">책이름</th>
            <th scope="col">저자</th>
            <th scope="col">가격</th>
          </tr>
        </thead>
        <tbody>
        <% for(let book of book_info){ %>
          <tr>
            <th scope="row"><%= book_info.indexOf(book) + 1 %></th>
            <td><a href="/book/bookDetail/<%= book.book_uid %>"><%= book.book_name %></a></td>
            <td><%= book.book_writer %></td>
            <td><%= book.book_price %>원</td>
          </tr>
        <%}%>
        </tbody>
      </table><table class="table table-striped">
        ...
      </table>
</div>
<div class='container mt-5'>
    <!-- table -->
    <div class='container'>
        <table class="table table-striped">
            <thead>
            <tr>
                <th scope="col">주문번호</th>
                <th scope="col">주문일자</th>
                <th scope="col">주문금액</th>
                <th scope="col">포인트사용금액</th>
                <th scope="col"></th>
          </tr>
            </tr>
            </thead>
            <tbody>
            <% for(let order of order_info){ %>
            <tr>
                <th  style='vertical-align: middle'><a href=''><%= order.order_uid %></a></th>
                <td  style='vertical-align: middle'><%= order.order_date %></a></td>
                <td  style='vertical-align: middle'><%= order.order_amount %></td>
                <td  style='vertical-align: middle'><%= order.point %></td>
                <td>
                    <div class="btn-group" role="group" aria-label="Basic outlined example">
                        <button type="button" class="btn btn-outline-primary" onclick="location.href='/order/deleteOrder/<%= order.order_uid %>'">삭제</button>
                    </div>
                </td>
            </tr>
            <% } %>
            </tbody>
        </table><table class="table table-striped">
        </table>
    </div>
</div>
<!doctype html>
<html lang="en">
  <head>

    <!-- head -->
    <%- include('./partials/head') %>

  </head>
  <body>
    
    <!-- navbar -->
    <%- include('./partials/nav') %>

    <!-- text -->
    <div class='text-center'>
        <h1 class='display-1 mt-5 mb-4'>Hello, world!</h1>
    </div>

    <!-- signin -->
    <div class='w-50 container mt-5'>
      <form>
        <div class="col-auto mb-3">
          <input type="text" class="form-control" placeholder="아이디" style='height: 65px';>
        </div>
        <div class="col-auto mb-3">
          <input type="password" class="form-control" placeholder="비밀번호" style='height: 65px';>
        </div>
        <div class="col-auto">
          <button type="submit" class="w-100 btn btn-primary btn-lg" style='height: 65px';>로그인</button>
        </div>
      </form>
    </div>


    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS -->

<!doctype html>
<html lang="en">
  <head>
    
    <!-- head -->
    <%- include('../partials/head') %>

  </head>
  <body>
    
    <!-- navbar -->
    <%- include('../partials/nav') %>

    <!-- signup -->
    <div class='w-50 container mt-5'>
        <form id='frm' action='/user/signup' method='post'>
          <div class="col-auto mb-3">
            <label class="col-form-label">이름</label>
            <input name='user_name' data-name='이름'type="text" class="form-control" required>
          </div>
          <div class="col-auto mb-3">
            <label class="col-form-label">아이디</label>
            <input name='user_uid' data-name='아이디' type="text" class="form-control" required>
          </div>
          <div class="col-auto mb-5">
            <label class="col-form-label">비밀번호</label>
            <input name='user_password' data-name='비밀번호' type="password" class="form-control" required>
          </div>
          <div class="col-auto">
            <button type="submit" class="w-100 btn btn-primary btn-lg" style='height: 65px';>회원가입</button>
          </div>
        </form>
    </div>


    <!-- Optional JavaScript; choose one of the two! -->

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-U1DAWAznBHeqEIlVSCgzq+c9gqGAJn5c/t99JyeKa9xxaYpSvHU5awsuZVVFIhvj" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS -->
    <!--
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.3/dist/umd/popper.min.js" integrity="sha384-eMNCOe7tC1doHpGoWe/6oMVemdAVTMs2xqW4mwXrXsW0L84Iytr2wi5v2QjrP/xp" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.min.js" integrity="sha384-cn7l7gDp0eyniUwwAZgrzD06kc/tftFf19TOAs2zVinnD/C7E91j9yyk5//jjpt/" crossorigin="anonymous"></script>
    -->
  </body>
</html>